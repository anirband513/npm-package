{{/hasRouter}}
{{#hasRedux}}
import { Provider } from 'react-redux'
import { store } from './store'
{{/hasRedux}}
{{#hasReactQuery}}
import { QueryClient, QueryClientProvider } from '@tanstack/react-query'
import { ReactQueryDevtools } from '@tanstack/react-query-devtools'
{{/hasReactQuery}}
import App from './App'
{{#hasTailwind}}
import './index.css'
{{/hasTailwind}}
{{^hasTailwind}}
{{#hasScss}}
import './index.scss'
{{/hasScss}}
{{^hasScss}}

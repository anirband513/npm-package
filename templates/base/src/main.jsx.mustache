    {{#hasReactQuery}}
    <QueryClientProvider client={queryClient}>
    {{/hasReactQuery}}
    {{#hasRedux}}
    <Provider store={store}>
    {{/hasRedux}}
    {{#hasRouter}}
    <BrowserRouter>
    {{/hasRouter}}
      <App />
    {{#hasRouter}}
    </BrowserRouter>
    {{/hasRouter}}
    {{#hasRedux}}
    </Provider>
    {{/hasRedux}}
    {{#hasReactQuery}}
      <ReactQueryDevtools initialIsOpen={false} />
